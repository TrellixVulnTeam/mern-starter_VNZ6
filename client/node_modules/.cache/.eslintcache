are className=\"hover mr-3\" onClick={toggleEditMode} />\n              <Trash onClick={handleDelete} className=\"hover mr-3\" />\n            </div>\n  }\n\n  const toggleEditMode = () => {setEditMode(!editMode);}\n\n  if (editMode){\n    return <UpdateForm \n            issue={issue} \n            editMode={editMode}\n            toggleEditMode={toggleEditMode}\n             />\n  }\n  return (\n    <div classsName=\"issue-list-item\">\n      <div className=\"card\">\n        <div className=\"card-header\" id={`heading-${issue._id}`}>\n          <h5 className=\"mb-0\">\n            <button className=\"btn btn-link\" data-toggle=\"collapse\" data-target={`#collapse-${issue._id}`} aria-expanded=\"true\" aria-controls={`collapse-${issue._id}`}>\n            {renderButton()} {issue.title} \n            </button>\n            <div className=\"float-right\">\n              {renderAuthorButtons()}\n            </div>\n          </h5>\n        </div>\n        <div id={`collapse-${issue._id}`} className=\"collapse\" aria-labelledby={`heading-${issue._id}`} data-parent={parentDiv}>\n        <div className=\"card-body\">\n          <small><strong>{issue.userUsername}</strong> @ {issue.timestamp}</small>\n          <div className=\"row\">\n            <div className=\"col-2\">\n              <img className=\"rounded-circle issue__user-image\" src=\"/temp-image.jpeg\" alt=\"\"/>\n            </div>\n            <div className=\"col-10\">\n              {issue.body}\n            </div>\n          </div>\n\n          </div>\n          <CommentsList parent={issue} type=\"issue\"/>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default IssueListItem;\n","/Users/mymac/repos/issue-tracker-mern/client/src/components/issues/UpdateForm.js",[],"/Users/mymac/repos/issue-tracker-mern/client/src/components/stickyMessages/UpdateForm.js",[],"/Users/mymac/repos/issue-tracker-mern/client/src/components/stickyMessages/MyEditor.js",["299"],"/Users/mymac/repos/issue-tracker-mern/clie